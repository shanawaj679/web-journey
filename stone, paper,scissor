    <script> 
   
    function rock(){
         let a,b,c,d;
        b=document.getElementById("rohan").innerText;
        a =Math.random();
        if(a<1/3){
          c ="ROCK";
        }
        else if(a>1/3 && a< 2/3){
            c="SCISSOR";
        }
        else{
            c="PAPER";
        }
    
     if(b === c){
          console.log("you clicked ",b,"and computer chose",c,"oh, it's TIE");
         document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — oh, it's TIE`;
     }
     else if(c === "PAPER"){
        console.log("you clicked ",b,"and computer chose",c,"alas, you LOOSE");
         document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — alas you loose`;
     }
     else if(c === "SCISSOR"){
         console.log("you clicked ",b,"and computer chose",c,"yeh, you WIN");
          document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — hurray you win`;
     }

    }
     function paper(){
         let a,b,c;
        b=document.getElementById("papu").innerText;
        a =Math.random();
        if(a<1/3){
          c ="ROCK";
        }
        else if(a>1/3 && a< 2/3){
            c="SCISSOR";
        }
        else{
            c="PAPER";
        }
          if(b === c){
          console.log("you clicked ",b,"and computer chose",c,"oh, it's TIE");
           document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — oh, it's TIE`;
     }
     else if(c === "ROCK"){
        console.log("you clicked ",b,"and computer chose",c,"yesss, you WIN");
         document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — yeeess, you win`;
     }
     else if(c === "SCISSOR"){
         console.log("you clicked ",b,"and computer chose",c,"ohhhh, you LOOSE");
          document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — it's fate had to loose`;
     }
     
    }
     function scissor(){
         let a,b,c;
        b=document.getElementById("sisu").innerText;
        a =Math.random();
        if(a<1/3){
          c ="ROCK";
        }
        else if(a>1/3 && a< 2/3){
            c="SCISSOR";
        }
        else{
            c="PAPER";
        }
      if(b === c){
          console.log("you clicked ",b,"and computer chose",c,"oh, it's TIE");
           document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — oh, it's TIE`;
     }
     else if(c === "PAPER"){
        console.log("you clicked ",b,"and computer chose",c,"alas, you WIN");
         document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — oh, i won`;
     }
     else if(c === "ROCK"){
         console.log("you clicked ",b,"and computer chose",c,"yeh, you LOOSE");
          document.getElementById("imp").innerText = `You clicked ${b} and computer chose ${c} — eww i loose`;
     }
